{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/apps/art/(.*)",
      "destination": "/apps/art/$1"
    },
    {
      "source": "/apps/quiz/(.*)",
      "destination": "/apps/quiz/$1"
    },
    {
      "source": "/apps/game/(.*)",
      "destination": "/apps/game/$1"
    },
    {
      "source": "/img/(.*)",
      "destination": "/img/$1"
    },
    {
      "source": "/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/doc/(.*)",
      "destination": "/doc/$1"
    },
    {
      "source": "/art/(.*)",
      "destination": "/apps/art/$1"
    },
    {
      "source": "/art",
      "destination": "/apps/art/index.html"
    },
    {
      "source": "/quiz/(.*)",
      "destination": "/apps/quiz/$1"
    },
    {
      "source": "/game/(.*)",
      "destination": "/apps/game/$1"
    },
    {
      "source": "/game",
      "destination": "/apps/game/index.html"
    },
    {
      "source": "/",
      "destination": "/apps/quiz/quiz2.html"
    },
    {
      "source": "/(style\\.css|app\\.js|manifest\\.json|sw\\.js|icon\\.png|quiz_data\\.json|ranking\\.json)",
      "destination": "/apps/quiz/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/apps/quiz/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*\\.css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/(.*\\.js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.*\\.(png|jpg|jpeg|gif|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/manifest\\.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ],
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "api/**/*.go": {
      "runtime": "vercel-go@latest"
    },
    "apps/quiz/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  }
}
