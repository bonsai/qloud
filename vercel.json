{
  "functions": {
    "quiz/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/quiz/quiz2.html"
    },
    {
      "source": "/rank/api",
      "destination": "/quiz/rank/ranking.php"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}